    "mkdirp": "^0.5.0",
    "native-dns": "^0.7.0",
    "network-address": "^1.0.0",
    "opn": "^0.1.2",
    "respawn": "^0.4.2",
    "rimraf": "^2.2.8",
    "shell-quote": "^1.4.2",
    "shelljs": "^0.3.0",
    "tildify": "^0.2.0",
    "touch": "0.0.3",
    "update-notifier": "^0.2.0",
    "xtend": "^3.0.0"
  },
  "devDependencies": {
    "mocha": "^1.21.4",
    "request": "^2.45.0",
    "supertest": "^0.13.0",
    "ws": "^0.7.0"
  },
  "license": "MIT"
}
